<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:scrollbars="vertical"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:id="@+id/rlayout"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:background="@color/black_overlay"
        android:paddingBottom="5dp"
        android:paddingEnd="5dp"
        android:paddingLeft="5dp"
        android:paddingTop="5dp">


        <TextView
            android:id="@+id/tvName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@android:color/white" />


        <TextView
            android:id="@+id/tvCranialL_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvName"
            android:paddingRight="15dp"
            android:text="Cranial limit: "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvCranialL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvName"
            android:layout_toRightOf="@id/tvCranialL_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />


        <TextView
            android:id="@+id/tvCaudal_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvCranialL_static"
            android:paddingRight="15dp"
            android:text="Caudal limit: "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvCaudalL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvCranialL"
            android:layout_toRightOf="@id/tvCaudal_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />


        <TextView
            android:id="@+id/tvAnteriorL_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvCaudal_static"
            android:paddingRight="9dp"
            android:text="Anterior limit: "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvAnteriorL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvCaudalL"
            android:layout_toRightOf="@id/tvAnteriorL_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />


        <TextView
            android:id="@+id/tvPosteriorL_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvAnteriorL_static"
            android:paddingRight="2dp"
            android:text="Posterior limit: "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvPosteriorL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvAnteriorL"
            android:layout_toRightOf="@id/tvPosteriorL_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />


        <TextView
            android:id="@+id/tvMedialL_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvPosteriorL_static"
            android:paddingRight="17dp"
            android:text="Medial limit: "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvMedialL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvPosteriorL"
            android:layout_toRightOf="@id/tvMedialL_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/tvLateralL_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvMedialL_static"
            android:paddingRight="16dp"
            android:text="Lateral limit: "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />


        <TextView
            android:id="@+id/tvLateralL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvMedialL"
            android:layout_toRightOf="@id/tvLateralL_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/tvComment_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvLateralL_static"
            android:paddingRight="19dp"
            android:text="Comments : "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvComment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvLateralL"
            android:layout_toRightOf="@id/tvComment_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/tvRisk_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvComment_static"
            android:paddingRight="19dp"
            android:text="Risk of Toxicity : "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvRisk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvComment"
            android:layout_toRightOf="@id/tvRisk_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/tvConstraints_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvRisk"
            android:paddingRight="19dp"
            android:text="Plan. Constraints : "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvConstraints"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvRisk"
            android:layout_toRightOf="@id/tvConstraints_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/tvOtherConstraints_static"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvConstraints_static"
            android:paddingRight="19dp"
            android:text=" Other Constraints : "
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/holo_blue_dark" />

        <TextView
            android:id="@+id/tvOtherConstraints"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvConstraints"
            android:layout_toRightOf="@id/tvOtherConstraints_static"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@android:color/white" />


    </RelativeLayout>

</ScrollView>